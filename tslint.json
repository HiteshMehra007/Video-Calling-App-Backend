{
    "defaultSeverity": "error",
    "extends": [
      "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "no-console": {
        "severity": "warning",
        "options": ["debug", "info", "trace"]
      },
      "max-line-length": [ true, 100],
      "semicolon": {
        "options": ["always"]
      }
    },
    "rulesDirectory": []
  }
  